node {
    
        stage('Breakfast') {
            echo 'I am having a breakfast.'
        }
        stage('Workout') {
            echo 'I am working out.'
        }
        stage('Study') {
            echo 'I am studying.'
        }
        stage('Family Time') {
            echo 'I am spending time with my family.'
        }
        stage('Play') {
            echo 'I am playing.'
        }
        if (currentBuild.result == null || currentBuild.result == 'SUCCESS') {
            echo 'My day went well.'
        }
        else{
        echo'did not went well'
    }
    
}
